<?xml version='1.0' encoding='UTF-8'?>	<project basedir='.' default='build' name='CICDJavaSample'>	    <property name='debuglevel' value='source,lines,vars'/>	    <property name='target' value='1.5'/>	    <property name='source' value='1.5'/>	    <property name='warname' value='mywar.war'/>	    <property name='gcf' value='/usr/bin/gcf' />	    <path id='classpathDir'>	        <pathelement location='bin'/>	        <pathelement location='lib/servlet-api.jar' />	    </path>	    <target name='init'>	        <mkdir dir='bin'/>	        <copy includeemptydirs='false' todir='bin'>	            <fileset dir='src'>	                <exclude name='**/*.java'/>	            </fileset>	        </copy>	        <copy includeemptydirs='false' todir='bin'>	            <fileset dir='WebContent'>	            </fileset>	        </copy>	    </target>	    <target name='clean'>	        <delete dir='bin'/>	    </target>	    <target name='build' depends='login,build-project,build-war,push-war,logout'/>	    <target name='cleanall' depends='clean'/>	    <target name='build-project' depends='init'>	        <echo message='${ant.project.name}: ${ant.file}'/>	        <javac debug='true' debuglevel='${debuglevel}' destdir='bin' source='${source}' target='${target}'			includeantruntime='false'>	            <src path='src'/>	            <classpath refid='classpathDir'/>	        </javac>	    </target>	   <target name='build-war' depends='build-project'>			<war destfile='${warname}' basedir='bin' />	    </target>	   <target name='push-war'>	        <exec dir='${basedir}' executable='${gcf}' failonerror='true'>	            <arg line='push -n ${ant.project.name} -p ${warname} ${ant.project.name}'/>	        </exec>	    </target>	    <target name='login'>	        <exec dir='${basedir}' executable='${gcf}' failonerror='true'>	            <arg line='login -a ${cf.apiEndpoint} -u ${cf.userName} -p ${cf.password} -o ${cf.org} -s ${cf.space}'/>	        </exec>	    </target>	    <target name='logout'>	        <exec dir='${basedir}' executable='${gcf}' failonerror='true'>	            <arg line='logout'/>	        </exec>	    </target>	</project>